set output "hr_3.png"
plot "hr_3.dat" using 1:2 title 'ARGON2'
set output "hr_4.png"
plot "hr_4.dat" using 1:2 title 'X17'
set output "hr_5.png"
plot "hr_5.dat" using 1:2 title 'LYRA2REv2'
set output "hr_6.png"
plot "hr_6.dat" using 1:2 title 'EQUIHASH'
set output "hr_7.png"
plot "hr_7.dat" using 1:2 title 'CRYPTONIGHT'

